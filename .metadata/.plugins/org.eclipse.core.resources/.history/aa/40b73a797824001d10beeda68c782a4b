
public class Hamburger {
	private String name;
	private String meat;
	private double price;
	private String breadRollType;
	
	private String addition1Name;
	private String addition1Price;
	private String addition2Name;
	private String addition2Price;
	private String addition3Name;
	private String addition3Price;
	private String addition4Name;
	private String addition4Price;
	
	public Hamburger(String name, String meat, double price, String breadRollType) {
		super();
		this.name = name;
		this.meat = meat;
		this.price = price;
		this.breadRollType = breadRollType;
	}
	
	public void addHamburgerAddition1(String name, String Price) {
		this.price + addition1Price
	}
	public void addHamburgerAddition2(String name, String Price) {
		this.price + addition2Price
	}
	public void addHamburgerAddition3(String name, String Price) {
		this.price + addition3Price
	}
	public void addHamburgerAddition4(String name, String Price) {
		this.price + addition4Price
	}

	
}
