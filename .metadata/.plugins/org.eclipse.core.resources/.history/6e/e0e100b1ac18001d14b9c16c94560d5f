
public class ComplexNumber {

	private double imaginary;
	private double real;
	
	
	
	public ComplexNumber(double imaginary, double real) {
		 this.imaginary = imaginary;
		 this.real = real;
	}



	public double getImaginary() {
		return imaginary;
	}



	public void add(double imaginary, double real) {
		this.real = real+imaginary;
	}
	
	public double add(ComplexNumber a) {
		
		return real+imaginary;
	}



	public double getReal() {
		return real;
	}



	public double subtract(double real, double imaginary) {
		this.imaginary = imaginary;
		this.real = real;
		return	real-imaginary;
	}
	
	public double subtract(ComplexNumber b) {
		return real-imaginary;
	}
	
	
	
	
}
